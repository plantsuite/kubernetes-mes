{
    "ServiceName": "plantsuite-account",
    "Database": {
        "MongoDb": {
            "DatabaseName": "ps-account"
        }
    },
    "JwtOptions": {
        "Issuer": "PlantSuite.Account",
        "Audience": "http://account.plantsuite.svc.cluster.local",
        "ExpirationMinutes": 300
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://0.0.0.0:80"
            }
        },
        "Limits": {
            "MaxRequestHeadersTotalSize": 131072
        }
    },
    "MessageBus": {
        "RabbitMQ": {
            "Host": "rabbitmq.rabbitmq.svc.cluster.local",
            "Port": 5672,
            "RetryCount": 5,
            "RetryInterval": 5000,
            "VirtualHost": "/",
            "SetQuorumQueue": true
        }
    },
    "Otlp": {
        "Endpoint": "http://otel-collector.monitoring.svc.cluster.local:4317",
        "Metrics": {
            "AddCoreAppMetrics": false,
            "Enabled": false
        },
        "Tracing": {
            "Enabled": false,
            "SamplerProbability": 0.1
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    }
}