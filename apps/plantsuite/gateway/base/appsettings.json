{
    "ServiceName": "plantsuite-gateway",
    "InstanceId": "",
    "InstanceName": "",
    "Database": {
        "MongoDb": {
            "DatabaseName": "ps-gateway"
        }
    },
    "JwtOptions": {
        "Audience": "ps-mes-portal",
        "Authority": "http://account.plantsuite.svc.cluster.local",
        "RequireHttpsMetadata": false
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://0.0.0.0:80"
            }
        },
        "Limits": {
            "MaxRequestHeadersTotalSize": 131072
        }
    },
    "MessageBus": {
        "RabbitMQ": {
            "Host": "rabbitmq.rabbitmq.svc.cluster.local",
            "Port": 5672,
            "RetryCount": 5,
            "RetryInterval": 5000,
            "VirtualHost": "/",
            "SetQuorumQueue": true,
            "MaxPendingMessages": 100000,
            "RedeliveryCount": 3,
            "RedeliveryInterval": 120
        },
        "MQTT": {
            "Host": "vernemq.vernemq.svc.cluster.local",
            "Port": 1883,
            "MaxPendingMessages": 10,
            "AutoReconnectDelay": 500,
            "PendingMessagesOverflowStrategy": "DropOldestQueuedMessage",
            "WebSocketPort": 15675
        }
    },
    "Otlp": {
        "Endpoint": "http://otel-collector.monitoring.svc.cluster.local:4317",
        "Metrics": {
            "AddCoreAppMetrics": false,
            "Enabled": false
        },
        "Tracing": {
            "Enabled": false,
            "SamplerProbability": 0.1
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    },
    "MonitorSettings": {
        "Interval": 15000
    },
    "CacheSettings": {
        "SlidingExpiration": 60
    }
}